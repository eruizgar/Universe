{
"id": "Ingestion",
"cpus": 1,
"mem": 128,
"instances": 1,
"env": {
"APPLICATION_WEB_PROXY_BASE": "/service/ingestion"
},
"container": {
"type": "DOCKER",
"docker": {
"image": "stratio/ingestion",
"network": "HOST"
}
},
"healthChecks": [
{
"path": "/",
"portIndex": 1,
"protocol": "HTTP",
"gracePeriodSeconds": 5,
"intervalSeconds": 60,
"timeoutSeconds": 10,
"maxConsecutiveFailures": 3
}
],
"labels": {
"DCOS_PACKAGE_FRAMEWORK_NAME": "Ingestion"
}
}